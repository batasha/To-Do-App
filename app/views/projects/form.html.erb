<%= form_for(@project) do |f| %>
	<div class="field">
		<%= f.label :title %><br>
		<%= f.text_field :title %>
	</div>

	<div class="field">
		<%= f.label :description %><br>
		<%= f.text_area :description %>
	</div>

	<div class="field">
		<%= f.label :team %>
		<%= f.collection_select :team_id, Team.all, :id, :name %>
	</div>

	<%= f.submit %>
<% end %>
